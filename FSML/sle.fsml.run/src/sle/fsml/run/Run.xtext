grammar sle.fsml.run.Run with org.eclipse.xtext.common.Terminals

generate run "http://www.fsml.sle/run/Run"
import "http://www.fsml.sle/input/Input" as input
import "http://www.fsml.sle/FSML" as fsml

Runfile:
	{Runfile} runs+=Run+;

Run:
	'run' machine=STRING 'on' input=STRING 'to' target=STRING;